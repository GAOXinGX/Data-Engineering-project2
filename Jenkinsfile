
pipeline{
  agent any
  
  stages {
    stage('Build'){
      steps{
       echo Building App
        }
      }
    }
    stage('Run'){
      steps{
       echo running
        }
      }
    }    
    stage('Test'){
      steps{
       echo Testing
        }
      }
    }    
    stage('Finish'){
      steps{
       echo Success
        }
      }
    }
    }
}